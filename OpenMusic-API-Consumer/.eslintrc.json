{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-unused-expressions": "off",
    "no-underscore-dangle": "off",
    "no-sequences": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "class-methods-use-this": "off",
    "max-len": "off"
  }
}
